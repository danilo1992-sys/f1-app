---
import { Race } from '../api/NextRace.ts'

const data = await Race()
const circuitName = data[0]?.circuit?.circuitName
console.log(circuitName, 'ok')
const token = import.meta.env.API_KEY_TOKEN

const options = {
  method: "GET",
  redirect: "follow"
  };

  const url = `https://google.serper.dev/images?q=${circuitName}&apiKey=${token}`
  const response = await fetch(url, options)
  const result = await response.json()
  const img = result.images[0].imageUrl
  console.log(img,'ok')
  ---
  <img 
  class="w-xl pl-50 pt-6"
  src={img} >



